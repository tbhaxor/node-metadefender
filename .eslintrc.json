{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["google"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "require-jsdoc": "off",
    "max-len": "off",
    "space-before-function-paren": "off",
    "camelcase": "off",
    "indent": "off",
    "comma-dangle": "off"
  }
}
